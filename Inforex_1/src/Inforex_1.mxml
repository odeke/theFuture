					var buyTime:String = buy_dateHour.textDisplay.text + ":" + buy_dateMinute.textDisplay.text + ":00 " + buy_am_pm.selectedItem;
					var buyDateTime:String  = dateFormatterAuctions.format(buy_expiry_date.selectedDate) + " " + buyTime;
